{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "semi": [2, "never"],
    "comma-dangle": [2, "always-multiline"],
    "no-class-assign": [0],
    "no-func-assign": [0]
  }
}
